@article{sironenMoreNoncompensatorySustainable2014,
  title = {Towards More Non-Compensatory Sustainable Society Index},
  author = {Sironen, Susanna and Sepp{\"a}l{\"a}, Jyri and Leskinen, Pekka},
  year = {2014},
