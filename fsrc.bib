  shorttitle = {Why {{Do We Need Food Systems Informatics}}?},
  author = {Tomich, Thomas P. and Hoy, Casey and Dimock, Michael R. and Hollander, Allan D. and Huber, Patrick R. and Hyder, Ayaz and Lange, Matthew C. and Riggle, Courtney M. and Roberts, Michael T. and Quinn, James F.},
