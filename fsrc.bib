  langid = {english},
  keywords = {Computers / Artificial Intelligence / General,Computers / Computer Science,Computers / Data Science / Data Analytics,Computers / Database Administration & Management,Computers / Internet / General,Computers / System Administration / Storage & Retrieval,Computers / Web / General,Language Arts & Disciplines / Library & Information Science / Cataloging & Classification},
  file = {C\:\\Users\\cdonov12\\Zotero\\storage\\CPKBC5UX\\Heath and Bizer - 2011 - Linked Data Evolving the Web Into a Global Data S.pdf;C\:\\Users\\d1033\\OneDrive - University of Vermont\\Documents\\Svalbard\\Books\\FLOSS and Semantic Web\\(Synthesis Lectures on Web Engineering) Tom Heath, Christian Bizer - Linked Data-Morgan & Claypool Publishers (2011).pdf}
