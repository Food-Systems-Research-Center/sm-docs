@article{baconSocialDimensionsSustainability2012,
  title = {The {{Social Dimensions}} of {{Sustainability}} and {{Change}} in {{Diversified Farming Systems}}},
  author = {Bacon, Christopher M. and Getz, Christy and Kraus, Sibella and Montenegro, Maywa and Holland, Kaelin},
